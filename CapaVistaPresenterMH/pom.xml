<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>MH</groupId>
  <artifactId>CapaVistaPresenterMH</artifactId>
  <packaging>war</packaging>
  <version>0.0.1-SNAPSHOT</version>
  <name>CapaVistaPresenterMH Maven Webapp</name>
  <url>http://maven.apache.org</url>
  <dependencies>
  
	<!-- pruebas unitarias -->  
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
    
    <!-- Servlet API -->
    <dependency>
    	<groupId>javax.servlet</groupId>
    	<artifactId>servlet-api</artifactId>
    	<version>3.0-alpha-1</version>
    </dependency>
    
    <!-- RESTEasy framework Dependecies -->
    <dependency>
		<groupId>org.jboss.resteasy</groupId>
		 <artifactId>resteasy-jaxrs</artifactId>
		<version>3.0.7.Final</version>
	</dependency>	
	
	<dependency>           
          <groupId>org.jboss.resteasy</groupId>
            <artifactId>jaxrs-api</artifactId>
            <version>3.0.7.Final</version>
    </dependency>
    <!-- necesaria para iniciar el servlet y aceptar solicitudes de la web -->
    <dependency>
            <groupId>org.jboss.resteasy</groupId>
            <artifactId>resteasy-servlet-initializer</artifactId>
            <version>3.0.7.Final</version>
     </dependency>  
     <!-- biblioteca cliente web service jax rs 2.0 -->
     <dependency>           
          <groupId>org.jboss.resteasy</groupId>
            <artifactId>resteasy-client</artifactId>
            <version>3.0.7.Final</version>
    </dependency>
    
    <!-- JAXB dependencies-->
	<dependency>
	    <groupId>org.jboss.resteasy</groupId>
	    <artifactId>resteasy-jaxb-provider</artifactId>
	    <version>3.0.7.Final</version>
	</dependency>
    
    <!-- Struts2 dependecies -->
    <dependency>
		<groupId>org.apache.struts</groupId>
		<artifactId>struts2-core</artifactId>
		<version>2.3.16.3</version>		
	</dependency>
    
    <!-- proyecto personalizado dependencia-->
    <dependency>
    	<groupId>MH</groupId>
    	<artifactId>ModeloDeRecursos</artifactId>
    	<version>0.0.1-SNAPSHOT</version>
    </dependency>
    
    <!-- Loggings -->
	<!-- Se define que debe omitirse el provisto por el framework (commons) -->
	<dependency>
  		<groupId>commons-logging</groupId>
  		<artifactId>commons-logging</artifactId>
  		<version>1.1.1</version>  		
	</dependency>
	
	<!-- utilizada la api de SLF4J -->
	<dependency>
		<groupId>org.slf4j</groupId>
		<artifactId>slf4j-api</artifactId>
		<version>1.7.7</version>
	</dependency>
	
	<!-- SLF4J: implementando "simple" -->	
	<dependency>
		<groupId>org.slf4j</groupId>
		<artifactId>slf4j-simple</artifactId>
		<version>1.7.7</version>
	</dependency>
    
    
  </dependencies>
  <build>
    <finalName>CapaVistaPresenterMH</finalName>
  </build>
</project>
